{
  "name": "immortal-engine-styles",
  "version": "0.1.0",
  "description": "Tailwind CSS styles for Immortal Engine",
  "private": true,
  "scripts": {
    "build": "npx @tailwindcss/cli -i ./assets/styles/input.css -o ./assets/styles/main.css",
    "watch": "npx @tailwindcss/cli -i ./assets/styles/input.css -o ./assets/styles/main.css --watch",
    "minify": "npx @tailwindcss/cli -i ./assets/styles/input.css -o ./assets/styles/main.css --minify"
  },
  "devDependencies": {
    "@tailwindcss/cli": "^4.1.0",
    "tailwindcss": "^4.1.0"
  }
}
